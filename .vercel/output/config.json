{
	"version": 3,
	"routes": [
		{
			"src": "/_app/immutable/.+",
			"headers": {
				"cache-control": "public, immutable, max-age=31536000"
			}
		},
		{
			"handle": "filesystem"
		},
		{
			"src": "^/adventures/([^/]+?)/([^/]+?)/?$",
			"dest": "/adventures/[path1]/[path2]?__pathname=/adventures/$1/$2"
		},
		{
			"src": "^/adventures/([^/]+?)/([^/]+?)/?/__data.json$",
			"dest": "/adventures/[path1]/[path2]/__data.json?__pathname=/adventures/$1/$2"
		},
		{
			"src": "/.*",
			"dest": "/fn"
		}
	],
	"overrides": {
		"index.html": {
			"path": ""
		}
	}
}